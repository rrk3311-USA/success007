{
  "installCommand": "npm install",
  "buildCommand": "npm run build:all",
  "outputDirectory": "deploy-site",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/product/:sku",
      "destination": "/product/index.html?sku=:sku"
    },
    {
      "source": "/faq/:sku",
      "destination": "/faq/index.html?sku=:sku"
    },
    {
      "source": "/life-jet",
      "destination": "/life-jet/dist/index.html"
    },
    {
      "source": "/workflow-builder",
      "destination": "/workflow-builder/index.html"
    },
    {
      "source": "/workflow-builder/*",
      "destination": "/workflow-builder/index.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/life-jet/dist/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
