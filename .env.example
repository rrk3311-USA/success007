# Walmart API Credentials
WALMART_CLIENT_ID=your_client_id_here
WALMART_CLIENT_SECRET=your_client_secret_here

# WooCommerce API Credentials
WOOCOMMERCE_URL=https://yoursite.com
WOOCOMMERCE_CONSUMER_KEY=your_consumer_key
WOOCOMMERCE_CONSUMER_SECRET=your_consumer_secret

# Stripe API Key
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# PayPal API Credentials (for PayPal Agent Toolkit)
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# Hostinger FTP/SFTP Deployment Credentials
HOSTINGER_HOST=ftp.yourdomain.com
HOSTINGER_USER=your_ftp_username
HOSTINGER_PASS=your_ftp_password
HOSTINGER_REMOTE_PATH=/public_html
# Optional: For SSH/rsync deployment
# HOSTINGER_SSH_KEY=/path/to/your/ssh/key
